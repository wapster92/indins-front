.youtube-modal
  max-width: 841px

  iframe
    border: none !important
    width: 100%
    height: 405px
    @media $display-breakpoints.sm-and-down
      height: 335px
    @media only screen and (max-width: 769px)
      height: 255px
    @media $display-breakpoints.xs-only
      height: 215px
    @media only screen and (max-width: 349px)
      height: 215px